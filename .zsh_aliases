# Usefully aliases
echo zsh_aliases loading...
alias zconfig="vim ~/.zshrc"
alias zcopy="cp -i ~/.zshrc ."
alias zupdate="cp -i .zshrc ~/.zshrc"
alias zreload="omz reload"
alias zaconfig="vim ~/.zsh_aliases"
alias zacopy="cp -i ~/.zsh_aliases ."
alias zaupdate="cp -i .zsh_aliases ~/.zsh_aliases"
alias zareload="source ~/.zsh_aliases"
alias vconfig="vim ~/.vimrc"
alias gconfig="vim ~/.gitconfig"
alias gitignore="vim .gitignore"
alias ohmyzsh="cd ~/.oh-my-zsh"
alias ohmyconfig="vim ~/.oh-my-zsh/oh-my-zsh.sh"
alias vin="vim +NERDTree"
alias vig="vim +NERDTree Gemfile"
alias th="theme"
alias tha="theme alexa"
alias thd="theme dhh"
alias thr="theme rvm"
alias lsth="lstheme"
alias rth="set | grep RANDOM_THEME"
alias jp="jump"
alias mk="mark"
alias mks="marks"
alias umk="unmark"
alias h="history"
alias update="sudo apt update && sudo apt upgrade -y && sudo apt autoremove -y"
echo zsh_aliases loaded.
